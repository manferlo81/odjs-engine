    "test": "jest --colors -w 3",
    "preversion": "npm run clean && npm test && npm run lint && npm run build"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "bundlib": "^0.13.0",
    "chokidar": "^3.1.1",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "files": [
    "dist",
    "types"
  ],
  "directories": {
    "test": "test"
  },
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
