{
  "name": "@odjs/engine",
  "version": "0.0.5",
  "description": "A param/option applier engine.",
  "keywords": [
    "odjs",
    "core",
    "param",
    "option",
    "applier",
    "appliers",
    "engine"
  ],
  "main": "dist/engine.cjs.js",
  "module": "dist/engine.es.js",
  "browser": "dist/engine.umd.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "bundlib": {
    "name": "odjs",
    "id": "odjs",
    "min": "browser"
  },
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "clean": "rm -rf node_modules/.cache dist types coverage",
