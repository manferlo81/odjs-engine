{
  "name": "@odjs/engine",
  "version": "0.0.5",
  "description": "A param/option applier engine.",
  "keywords": [
    "odjs",
    "core",
    "param",
    "option",
    "applier",
    "appliers",
    "engine"
  ],
  "main": "dist/engine.cjs.js",
  "module": "dist/engine.es.js",
  "browser": "dist/engine.umd.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "bundlib": {
    "name": "odjs",
    "id": "odjs",
    "min": "browser"
  },
  "scripts": {
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --colors -w 3",
    "clean": "rimraf dist types coverage node_modules/.cache/jest node_modules/.cache/bundlib",
    "lint": "eslint src/** __test__/**",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "bundlib": "^0.14.1",
    "chokidar": "^3.3.1",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist",
    "types"
  ],
  "directories": {
    "test": "test"
  },
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/odjs/engine.git"
  },
  "bugs": {
    "url": "https://github.com/odjs/engine/issues"
  },
  "homepage": "https://github.com/odjs/engine#readme",
  "license": "MIT"
}
